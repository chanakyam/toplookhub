  <div class="ad1 padding20_T"></div>
  <section class="contentarea padding20_T">
    <aside class="leftcontent">
      <div class="slideshow">           
      <div class="video" >
        <input type="hidden" name="video_val" id="video_val" value="{{videoParam.video}}">
        <input type="hidden" name="video_tit" id="video_tit" value="{{videoParam.title}}">
        <input type="hidden" name="video_desc" id="video_desc" value="{{videoParam.description}}">
        <input type="hidden" name="video_duration" id="video_duration" value="{{videoParam.duration}}">
          <div id="myElement">Loading the player...</div>
      </div>
      <span class="clearboth"></span>
      <div class="slideshow-content">       
        <p>{{ videoParam.description }} - {{ videoParam.upload_date }}</p>              
      </div>
    </div>
    <!--Slideshow VIDEOS End-->
    <span class="clear"></span>
      <div class="module-title margin20_T">POLITICS NEWS IN PICTURES</div>
       <div class="module padding10" style="margin-bottom: 34px;">        
        <div class="news">
          {% for n in thumbs %}
          <div class="news-row">
            <div class="newsthumb"><a href="/n/{{ n.id }}"><img src="http://img1.contentapi.ws/{{ n.thumb_image }}"/></a></div>
            <div class="news-right">
              <h3><a href="/n/{{ n.id }}">{{ n.title }}</a></h3>
                <p>{{ n.description }}</p>
                 <br>
                <span class="small">{{ n.upload_date }}</span></p>
            </div>
          </div>

          {% endfor %}
          <!-- news-row end -->
         </div>
        <span class="clear"></span> </div>
      <!-- module end -->
      <div class="module-title">LATEST POLITICS HEADLINES</div>
      <div class="module padding10">
      <div class="news">    
           {% for n in topnews1 %}
          <div class="row devider"> <a href="/n/{{ n.id }}">{{ n.title }}</a>
            <span class="small">{{ n.upload_date }}</span> 
          </div>         
          {% endfor %}
      </div>
          <span class="clearboth"></span>
